        <item name="android:textSize">16sp</item>
    </style>
    <style name="TextAppearance.Nightroll.Body2" parent="TextAppearance.Nightroll">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/secondary_text_color</item>
    </style>
    <style name="TextAppearance.Nightroll.Caption" parent="TextAppearance.Nightroll">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/secondary_text_color</item>
    </style>
    <style name="TextAppearance.Nightroll.Headline1" parent="TextAppearance.Nightroll">
        <item name="android:textSize">28sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TextAppearance.Nightroll.Headline2" parent="TextAppearance.Nightroll">
        <item name="android:textSize">24sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Theme.Nightroll" parent="Theme.Material3.DayNight.NoActionBar">
        
        <item name="colorPrimary">@color/primary_color</item>
        <item name="colorOnPrimary">@color/button_primary_text_color</item>
        <item name="colorPrimaryContainer">@color/primary_color</item>
        <item name="colorOnPrimaryContainer">@color/button_primary_text_color</item>
        
        
        <item name="colorSecondary">@color/secondary_text_color</item>
        <item name="colorOnSecondary">@color/text_color</item>
        <item name="colorSecondaryContainer">@color/card_background_color</item>
        <item name="colorOnSecondaryContainer">@color/text_color</item>
        
        
        <item name="colorTertiary">@color/border_color</item>
        <item name="colorOnTertiary">@color/text_color</item>
        <item name="colorTertiaryContainer">@color/card_background_color</item>
        <item name="colorOnTertiaryContainer">@color/text_color</item>
        
        
        <item name="colorError">@android:color/holo_red_dark</item>
        <item name="colorErrorContainer">@android:color/holo_red_light</item>
        <item name="colorOnError">@color/white</item>
        <item name="colorOnErrorContainer">@android:color/holo_red_dark</item>
        
        
        <item name="android:colorBackground">@color/background_color</item>
        <item name="colorOnBackground">@color/text_color</item>
        <item name="colorSurface">@color/card_background_color</item>
        <item name="colorOnSurface">@color/text_color</item>
        <item name="colorSurfaceVariant">@color/border_color</item>
        <item name="colorOnSurfaceVariant">@color/secondary_text_color</item>
        
        
        <item name="colorOutline">@color/border_color</item>
        <item name="colorOnSurfaceInverse">@color/background_color</item>
        <item name="colorSurfaceInverse">@color/text_color</item>
        <item name="colorPrimaryInverse">@color/primary_color</item>
        
        
        <item name="android:statusBarColor">@color/background_color</item>
        <item name="android:windowLightStatusBar">true</item>
        
        
        <item name="android:navigationBarColor">@color/card_background_color</item>
        <item name="android:windowLightNavigationBar">true</item>
        
        
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:enforceStatusBarContrast">false</item>
        <item name="android:enforceNavigationBarContrast">false</item>
    </style>
    <style name="Theme.Nightroll.Fullscreen" parent="Theme.Nightroll.NoActionBar">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@android:color/black</item>
    </style>
    <style name="Theme.Nightroll.NoActionBar" parent="Theme.Nightroll">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Widget.Nightroll.BottomNavigation" parent="Widget.Material3.BottomNavigationView">
        <item name="android:background">@color/card_background_color</item>
        <item name="itemIconTint">@color/primary_color</item>
        <item name="itemTextColor">@color/text_color</item>
        <item name="labelVisibilityMode">labeled</item>
    </style>
    <style name="Widget.Nightroll.Button" parent="Widget.Material3.Button">
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">8dp</item>
        <item name="android:minHeight">48dp</item>
    </style>
    <style name="Widget.Nightroll.Button.Primary" parent="Widget.Nightroll.Button">
        <item name="backgroundTint">@color/button_primary_background</item>
        <item name="android:textColor">@color/button_primary_text_color</item>
    </style>
    <style name="Widget.Nightroll.Button.Secondary" parent="Widget.Material3.Button.OutlinedButton">
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">8dp</item>
        <item name="android:minHeight">48dp</item>
        <item name="strokeColor">@color/border_color</item>
        <item name="android:textColor">@color/text_color</item>
    </style>
    <style name="Widget.Nightroll.Card" parent="Widget.Material3.CardView.Elevated">
        <item name="cardCornerRadius">12dp</item>
        <item name="cardElevation">2dp</item>
        <item name="cardBackgroundColor">@color/card_background_color</item>
        <item name="strokeColor">@color/border_color</item>
        <item name="strokeWidth">1dp</item>
    </style>
    <style name="Widget.Nightroll.FloatingActionButton" parent="Widget.Material3.FloatingActionButton.Primary">
        <item name="backgroundTint">@color/primary_color</item>
        <item name="tint">@color/button_primary_text_color</item>
    </style>
    <style name="Widget.Nightroll.SearchView" parent="Widget.AppCompat.SearchView">
        <item name="android:background">@drawable/search_background</item>
        <item name="queryBackground">@android:color/transparent</item>
        <item name="submitBackground">@android:color/transparent</item>
    </style>
    <style name="Widget.Nightroll.Toolbar" parent="Widget.Material3.Toolbar">
        <item name="android:background">@color/background_color</item>
        <item name="titleTextColor">@color/text_color</item>
        <item name="navigationIconTint">@color/text_color</item>
    </style>
